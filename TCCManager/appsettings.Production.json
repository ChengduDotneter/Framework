{
  "urls": "http://192.168.10.211:10003",

  //Consul中该服务的信息
  "ConsulService": {
    //该服务的ip
    "IP": "192.168.10.211",
    //该服务的服务名，此处应与网关配置中的微服务名对应
    "ServiceName": "TCCManager",
    //该服务的端口号
    "Port": "10003",
    //Consul的IP
    "ConsulIP": "192.168.10.211",
    //Consul的端口号
    "ConsulPort": "8500",
    //服务启动后的多少秒注册
    "DeregisterCriticalServiceAfter": 5,
    //心跳检测间隔
    "Interval": 10,
    //心跳检测接口路径
    "HealthPath": "health",
    //超时时间 （单位 5秒）
    "Timeout": 5
  }
}