<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.15</Version>
    <AssemblyVersion>2.0.15.0</AssemblyVersion>
    <FileVersion>2.0.15.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DefineConstants>TRACE;OUTPUT_ERROR_SQL;OUTPUT_SQL;DEBUG;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
    <DefineConstants>TRACE;OUTPUT_ERROR_SQL;NETSTANDARD2_0;RELEASE;NETSTANDARD;NETSTANDARD2_1</DefineConstants>
    <DocumentationFile>bin\Release\netstandard2.1\Common.DAL.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="sqlSugarCore" Version="5.0.0.15" />
    <PackageReference Include="MongoDB.Driver" Version="2.11.0" />
    <PackageReference Include="Apache.Ignite.Linq" Version="2.8.1" />
    <PackageReference Include="linq2db" Version="3.0.1" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.IO.FileSystem" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common.Log\Common.Log.csproj" />
    <ProjectReference Include="..\Common\Common.csproj" />
    <ProjectReference Include="..\Common.DAL.Transaction\Common.DAL.Transaction.csproj" />
  </ItemGroup>
</Project>
